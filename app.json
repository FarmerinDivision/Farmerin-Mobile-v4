{
  "expo": {
    "name": "Farmerin",
    "slug": "farmerinapps",
    "version": "4.1.0",
    "orientation": "portrait",
    "icon": "./assets/iconoFW.jpg",
    "splash": {
      "image": "./assets/LaktoFeeder.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF",
      "text": "AS"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ar.com.farmerin",
      "buildNumber": "45"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LaktoFeeder.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.farmerin.farmerin",
      "versionCode": 70,
      "permissions": [
        "CAMERA",
        "NOTIFICATIONS",
        "INTERNET"
      ],
      "usesCleartextTraffic": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "ndkVersion": "28.0.12674087",
            "useLegacyPackaging": false
          }
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1c4035a0-8f45-4a52-bee2-12698847d4b3"
      }
    },
    "owner": "farmerinfacup"
  }
}